<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>QueryStateServlet</servlet-name>
    <servlet-class>com.controller.QueryState</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QueryStateServlet</servlet-name>
    <url-pattern>/queryState</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QueryJsonServlet</servlet-name>
    <servlet-class>com.controller.QueryJsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QueryJsonServlet</servlet-name>
    <url-pattern>/queryjson</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>jqueryajax.jsp</welcome-file>
  </welcome-file-list>
</web-app>
